{{#each decks}}
<h3>{{Name}}</h3>
<form action="/editdeck" method="post">
<input type="hidden" name="name" value="{{Name}}">
<input type="submit" value="Edit This Deck"></input>
</form>
<div>

  <p><span class = "bold"> User: </span>{{User}}</p>
  <p><span class="bold"> Public: </span>{{Public}}</p>
</div>

<form action="/studydeck" method="post">
<input type="hidden" name="name" value="{{Name}}">
<input type="submit" value="Study This Deck"></input>
</form>

{{/each}}
